    "\n",
    "Y = df_gp[0:]\n",
    "Z = df_gp[1:]\n",
    "\n",
    "df = pd.DataFrame(np.c_[Y,Z], index=X)\n",
    "df.plot.bar()\n",
    "plt.labels(['0', '1'])\n",
    "\n",
    "plt.show()"
   ]
